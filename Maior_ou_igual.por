Algoritmo "Maior_de_dois"

Var
      n1, n2 : inteiro

Inicio

      escreval("escreva seu primeiro numero")
      leia(n1)
      escreval("escreva seu segundo numero")
      leia(n2)

      se (n1 < n2)entao
       escreval("O segundo numero é maior", n2)
       senao
      se (n1 > n2) entao
       Escreval ("O primeiro numero é maior", n1)
      senao
      escreval("São iguais")

       fimse
        fimse

Fimalgoritmo
