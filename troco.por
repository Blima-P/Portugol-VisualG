Algoritmo "troco"

Var
  preco, dinr, troco : real
  qtd : inteiro

Inicio

      Escreval("Preço unitário do produto: ")
      Leia(preco)
      Escreval("Quantidade comprada: ")
      Leia(qtd)
      Escreval("Dinheiro recebido :")
      leia (dinr)
      
                 Escreval()
      
      troco <- preco * qtd - dinr
      
      Escreval("Troco :", troco)
      
Fimalgoritmo